<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
	<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
	<%
	String path = request.getContextPath() + "/";
%>
<!DOCTYPE html>
<html>
<head>
<base href="<%=path%>" />
    <title>挂号-- -2015</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="Css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="Css/bootstrap-responsive.css" />
    <link rel="stylesheet" type="text/css" href="Css/style.css" />
    <script type="text/javascript" src="Js/jquery.js"></script>
    <script type="text/javascript" src="Js/jquery.sorted.js"></script>
    <script type="text/javascript" src="Js/bootstrap.js"></script>
    <script type="text/javascript" src="Js/ckform.js"></script>
    <script type="text/javascript" src="Js/common.js"></script>
    <script type="text/javascript" src="Js/ckeditor/ckeditor.js"></script>
 

    <style type="text/css">
        body {
            padding-bottom: 40px;
        }
        .sidebar-nav {
            padding: 9px 0;
        }

        @media (max-width: 980px) {
            /* Enable use of floated navbar text */
            .navbar-text.pull-right {
                float: none;
                padding-left: 5px;
                padding-right: 5px;
            }
        }


    </style>
    <script type="text/javascript">
    $(function () {       
		$('#backid').click(function(){
				window.location.href="registration/forward";
		 });
    });
    </script>
</head>
<body>
<form action="registration/addRegistration" method="post" class="definewidth m20">
<table class="table table-bordered table-hover definewidth m10">
    <tr>
        <td width="10%" class="tableleft">姓名</td>
        <td><input type="text" name="name" value="${registrationInfor2.name }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件类型</td>
        <td>
        	<select name="certificateType">
        		<c:forEach items="${certificateTypeList }" var="c">       		
        			<option ${registrationInfor2.certificateType.id == c.id? 'selected':'' } value="${c.id }">${c.name }</option>
        		</c:forEach> 
        	</select>
        </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件号</td>
        <td><input type="text" name="idNum" value="${registrationInfor2.idNum }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">社保号</td>
        <td><input type="text" name="socialSafeNum" value="${registrationInfor2.idNum }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">挂号费</td>
        <td><input type="number" name="registrationFee" value="${registrationInfor2.idNum }"/>元</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">联系电话</td>
        <td><input type="text" name="phone" value="${registrationInfor2.idNum }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">是否自费</td>
        <td>
        	<input ${registrationInfor2.selfPaying == 0? 'checked':'' } type="radio" name="selfPaying" value="0" />否&nbsp;&nbsp;&nbsp;
        	<input ${registrationInfor2.selfPaying == 1? 'checked':'' } type="radio" name="selfPaying" value="1"/>是
        </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">性别</td>
        <td><input ${registrationInfor2.sex == 1? 'checked':'' } type="radio" name="sex" value="1" checked/>男&nbsp;&nbsp;&nbsp;
        <input ${registrationInfor2.sex == 0? 'checked':'' } type="radio" name="sex" value="0"/>女</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">年龄</td>
        <td><input type="number" name="age" value="${registrationInfor2.age }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">职业</td>
        <td><input type="text" name="career" value="${registrationInfor2.career }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">初复诊</td>
        <td>
        	<input ${registrationInfor2.sex == 1? 'checked':'' } type="radio" name="earlyAppointment" value="初诊" checked/>初诊&nbsp;&nbsp;&nbsp;
        	<input ${registrationInfor2.sex == 0? 'checked':'' } type="radio" name="earlyAppointment" value="复诊"/>复诊
        </td>
    </tr> 
    <tr>
        <td width="10%" class="tableleft">所挂科室</td>
        <td>
        	<select name="seccoXuan.id">
        		<c:forEach items="${seccoXuanList }" var="s">
        			<option ${registrationInfor2.seccoXuan.id == s.id? 'checked':'' }  value="${s.id }">${s.seccoName }</option>
        		</c:forEach> 
        	</select>
        </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">指定医生</td>
        <td>
        	<select name="doctor.id">
        		<c:forEach items="${doctorList.list }" var="d">
        			<option ${registrationInfor2.doctor.id == d.id? 'checked':'' }  value="${d.id }">${d.name }</option>
        		</c:forEach> 
        	</select>
        </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">状态</td>
        <td>
        	<select name="status">
        		<option ${registrationInfor2.status == '已挂号'? 'checked':'' } value="已挂号">已挂号</option>
        		<option ${registrationInfor2.status == '已挂号'? 'checked':'' } value="已询医">已挂号</option>
        		<option ${registrationInfor2.status == '已挂号'? 'checked':'' } value="已出院">已挂号</option>
        		<option ${registrationInfor2.status == '已挂号'? 'checked':'' } value="已退号">已挂号</option>
        	</select>
        </td>
    </tr>
	<tr>
        <td width="10%" class="tableleft">备注</td>
        <td><textarea name="noted">${registrationInfor2.noted }</textarea></td>
	</tr>
    <tr>
        <td colspan="2">
			<center>
				<button type="submit" class="btn btn-primary" type="button">保存</button> &nbsp;&nbsp;<button type="button" class="btn btn-success" name="backid" id="backid">返回列表</button>
			</center>
		</td>
    </tr>
</table>
