<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<%@taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%
	String path = request.getContextPath() + "/";
%>
<!DOCTYPE html>
<html>
<head>
<base href="<%=path%>"/>
    <title>入院办理-- -2015</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="Css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="Css/bootstrap-responsive.css" />
    <link rel="stylesheet" type="text/css" href="Css/style.css" />
    <script type="text/javascript" src="Js/jquery.js"></script>
    <script type="text/javascript" src="Js/jquery.sorted.js"></script>
    <script type="text/javascript" src="Js/bootstrap.js"></script>
    <script type="text/javascript" src="Js/ckform.js"></script>
    <script type="text/javascript" src="Js/common.js"></script>
    <script type="text/javascript" src="laydate/laydate.js"></script>

    <style type="text/css">
        body {
            padding-bottom: 40px;
        }
        .sidebar-nav {
            padding: 9px 0;
        }

        @media (max-width: 980px) {
            /* Enable use of floated navbar text */
            .navbar-text.pull-right {
                float: none;
                padding-left: 5px;
                padding-right: 5px;
            }
        }


    </style>
    <script type="text/javascript">
	 $(function () {
		$('#newNav').click(function(){
				window.location.href="add.html";
		 });
    });
	
    	function checkall(){
			var alls=document.getElementsByName("check");
			var ch=document.getElementById("checkall");
			if(ch.checked){
				for(var i=0;i<alls.length;i++){
					alls[i].checked=true;	
				}	
			}else{
				for(var i=0;i<alls.length;i++){
					alls[i].checked=false;	
				}	
			}
		}
		function delAll(){
			var alls=document.getElementsByName("check");
			var ids=new Array();
			for(var i=0;i<alls.length;i++){
				if(alls[i].checked){
					ids.push(alls[i].value);
				}		
			}
			if(ids.length>0){
				if(confirm("确认操作?")){
					alert("成功!");
				}
			}else{
				alert("请选中要操作的项");
			}
		}
    </script>
</head>
<body>

<form action="hospitalization/vague" method="post" class="definewidth m20">
<table class="table table-bordered table-hover definewidth m10">
    <tr>
        <td width="10%" class="tableleft">病例号：</td>
        <td><input type="text" name="medicalRecord"/></td>
		
        <td width="10%" class="tableleft">主治医生：</td>
        <td><input type="text" name="doctorName"/></td>
		
        <td width="10%" class="tableleft">科室：</td>
        <td><input type="text" name="seccoName" /></td>
    </tr>
    <tr>
		
        <td width="10%" class="tableleft">住院时间：</td>
		
		  <td colspan="5">
			<input id="day1" type="text" name="time1"/>&nbsp;至&nbsp;<input onblur="proofTime()" id="day2" type="text" name="time2" />
			&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <button type="submit" class="btn btn-primary" type="button">查询</button> 
            <button type="submit" class="btn btn-primary" type="button">清空</button> 
			
        </td>
    </tr>
</table>
</form>
   
<table class="table table-bordered table-hover definewidth m10" >
   <thead>
    <tr>
    	<th><input type="checkbox" id="checkall" onChange="checkall();"></th>
        <th>病历号</th>
        <th>姓名</th>
        <th>床位号</th>
        <th>联系电话</th>
        <th>押金</th>
        <th>主治医生</th>
        <th>入院时间</th>
        <th>科室</th>
        <th>状态</th>
        <th>操作</th>
    </tr>
    </thead>	     
	     <c:forEach items="${hospitalInforList.list }" var="h" varStatus="status">
			<tr>
				<td style="vertical-align:middle;"><input type="checkbox" name="check" value="1"></td>
	            <td style="vertical-align:middle;">${h.medicalRecord }</td>
	            <td style="vertical-align:middle;">${h.registrationInfor.name }</td>
	            <td style="vertical-align:middle;">${h.bedId }</td>
	            <td style="vertical-align:middle;">${h.registrationInfor.phone }</td>
	            <td style="vertical-align:middle;">${h.payTheDeposit }</td>
	            <td style="vertical-align:middle;">${h.doctor.name }</td>
	            <td style="vertical-align:middle;">${h.registrationInfor.time }</td>
	            <td style="vertical-align:middle;">${h.seccoXuan.seccoName }</td>
	            <td style="vertical-align:middle;">${h.registrationInfor.status }</td>
				<td><a href="hospitalization/toEditPage?medicalRecord=${h.medicalRecord }&pageNum=${hospitalInforList.pageNum}">编辑</a>&nbsp;&nbsp;&nbsp;
					<a href="hospitalization/detail?medicalRecord=${h.medicalRecord }&pageNum=${hospitalInforList.pageNum}"	>详情</a>&nbsp;&nbsp;&nbsp;					
					<a href="javascript:void()" onclick="backCourt('${h.registrationInfor.status }','${h.medicalRecord}')">退院</a>&nbsp;&nbsp;&nbsp;
					<a href="javascript:void()" onclick="leaveHospital('${h.registrationInfor.status }','${h.medicalRecord}')">出院</a>
				</td>
			</tr>
		</c:forEach>
  </table>
  
  <table class="table table-bordered table-hover definewidth m10">
		<tr>
			<th colspan="5">
				<div class="inline pull-right page">
					${count }条记录 ${hospitalInforList.pageNum }/${hospitalInforList.pages } 页
					<c:if test="${hospitalInforList.isFirstPage }">
						<a href="javascript:void()" aria-label="Previous"> <span
							aria-hidden="true">&laquo;</span>
						</a>
					</c:if>
					<c:if test="${!hospitalInforList.isFirstPage }">
						<a href='hospitalization/forward?pageNum=${hospitalInforList.pageNum - 1 }'>&laquo;</a>
					</c:if>
					<c:forEach items="${hospitalInforList.navigatepageNums }" var="n">
						<!-- 判断是否是当前页,是导航页就是选中状态 -->
						<c:if test="${n==hospitalInforList.pageNum }">
							<a class="product-ym_bg" href="hospitalization/forward?pageNum=${n }">${n }</a>
						</c:if>
						<!-- 判断是否是当前页,是导航页就是非选中状态 -->
						<c:if test="${n!=hospitalInforList.pageNum}">
							<a class="product-ym_bg" href="hospitalization/forward?pageNum=${n }">${n }</a>
						</c:if>
					</c:forEach>
					<c:if test="${hospitalInforList.isLastPage }">

						<a href="javascript:void()" aria-label="Next"> <span
							aria-hidden="true">&raquo;</span>
						</a>

					</c:if>
					<!-- 判断是否是最后一页,是就不能点击 -->
					<c:if test="${!hospitalInforList.isLastPage }">

						<a class="product-ym_xyy"
							href="hospitalization/forward?pageNum=${hospitalInforList.pageNum + 1 }"
							aria-label="Next"> <span aria-hidden="true">&raquo;</span>
						</a>

					</c:if>
				</div>

				<div>
					<button onclick="addHospitalInfor()" type="button" class="btn btn-success">录入住院信息</button>&nbsp;&nbsp;&nbsp;
					 <button type="button" class="btn btn-success" id="delPro" onClick="delAll();">出院</button>&nbsp;&nbsp;&nbsp;
					 <button type="button" class="btn btn-success" id="delPro" onClick="delAll();">退院</button>&nbsp;&nbsp;&nbsp;
					 <button type="button" class="btn btn-success" id="delPro">导出Excel</button>
					 <button type="button" class="btn btn-success" id="delPro">打印</button>
				</div>
			</th>
		</tr>
	</table>
  
<script type="text/javascript">
   function addHospitalInfor() {
	   window.location.href="hospitalization/addHospitalInfor"
	}   
   
   function emptys() {
	   $("#input1").val(""); 
}
   
   
   function leaveHospital(status ,medicalRecord) {
	   if (status == "已出院") {
		alert("已出院,无需办理出院手续")
	}else if (status == "") {
		alert("状态未知,不可出院,可以退院")
	}else if (status == "已挂号" || status == "已退院") {
		alert("未结算,不可出院")
	} else if(status == "已结算"){
		$.ajax({
			type : 'POST',
			url : 'hospitalization/updateStatus',
    		async : true,
			data : {
				"medicalRecord" : medicalRecord,
				"status" : "已出院"
			},
			dataType : 'json',
			success : function(data) {
				alert("出院成功")
				window.location.href="hospitalization/forward"
			},
			error : function(msg) {
				alert("页面加载失败")
			}
		});
	}

}
   
   laydate.render({
		  elem: '#day1'
		});
   
   laydate.render({
		  elem: '#day2'
		});
   
   $("#day1").val()
   
   function proofTime() {
	  var time1 =  $("#day1").val()
	  var time2 = $("#day2").val()
	   if (time1 > time2) {
		alert("右边时间需要大于左边")
	}
}
